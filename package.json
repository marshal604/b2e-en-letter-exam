{
  "name": "english-letter-exam-backend",
  "version": "1.0.0",
  "description": "english-letter-exam server or api service",
  "main": "index.js",
  "engines": {
    "node": "11.9.0"
  },
  "scripts": {
    "start": "npm run serve:server",
    "start:server:dev": "npm run watch-serve",
    "serve:server": "cd server && ts-node -r tsconfig-paths/register src/server.ts",
    "build:server": "cd server && tsc",
    "watch-serve:server": "npm run watch-ts & npm run watch-node",
    "watch-node:server": "nodemon server/dist/server/server.js",
    "watch-ts:server": "cd server && tsc --watch",
    "substree:model": "GIT_SSL_NO_VERIFY=true git subtree pull --prefix server/src/gql-models gql-models master --squash"
  },
  "author": "hubert",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.1",
    "pg-promise": "^8.7.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/cheerio": "^0.22.10",
    "@types/node": "^10.12.18",
    "@types/pg-promise": "^5.4.3",
    "@types/ws": "^6.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.3.1",
    "ts-node": "^8.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}